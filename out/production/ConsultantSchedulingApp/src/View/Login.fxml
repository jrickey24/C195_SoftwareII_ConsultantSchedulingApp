<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="600.0" maxWidth="1200.0" minHeight="600.0" minWidth="1200.0" opacity="0.8" prefHeight="600.0" prefWidth="1200.0" styleClass="bodybg" stylesheets="@../Utils/BackgroundMain.CSS" xmlns="http://javafx.com/javafx/1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Login">
    <children>
        <Label fx:id="usernameLabel" layoutX="736.0" layoutY="261.0" opacity="0.95" text="%username" textAlignment="CENTER" textFill="#185296">
            <font>
                <Font name="Segoe UI Semibold" size="18.0" />
            </font>
        </Label>
        <Label fx:id="passwordLabel" layoutX="739.0" layoutY="324.0" opacity="0.95" text="%password" textFill="#185296">
            <font>
                <Font name="Segoe UI Semibold" size="18.0" />
            </font>
        </Label>
        <TextField fx:id="usernameTextField" blendMode="DARKEN" layoutX="918.0" layoutY="258.0" prefHeight="40.0" prefWidth="228.0">
            <font>
                <Font name="Segoe UI Semibold" size="14.0" />
            </font>
            <padding>
                <Insets bottom="0.1" left="0.1" right="0.1" top="0.1" />
            </padding>
            <opaqueInsets>
                <Insets bottom="0.8" left="0.8" right="0.8" top="0.8" />
            </opaqueInsets></TextField>
        <TextField fx:id="passwordTextField" blendMode="DARKEN" layoutX="918.0" layoutY="318.0" prefHeight="40.0" prefWidth="228.0">
            <font>
                <Font name="Segoe UI Semibold" size="14.0" />
            </font>
            <opaqueInsets>
                <Insets bottom="0.8" left="0.8" right="0.8" top="0.8" />
            </opaqueInsets>
         <padding>
            <Insets bottom="0.1" left="0.1" right="0.1" top="0.1" />
         </padding></TextField>
        <Button fx:id="loginSubmitButton" alignment="CENTER" contentDisplay="CENTER" layoutX="1056.0" layoutY="446.0" mnemonicParsing="false" onAction="#handleSubmitButton" prefHeight="42.0" prefWidth="109.0" text="%submit" textFill="#185296">
            <font>
                <Font name="Segoe UI Semibold" size="16.0" />
            </font>
        </Button>
        <Button fx:id="loginExitButton" alignment="CENTER" contentDisplay="CENTER" layoutX="905.0" layoutY="446.0" mnemonicParsing="false" onAction="#handleExitButton" prefHeight="42.0" prefWidth="115.0" text="%exit" textFill="#185296">
            <font>
                <Font name="Segoe UI Semibold" size="16.0" />
            </font>
        </Button>
      <Label fx:id="zoneIdLabel" layoutX="904.0" layoutY="32.0" textFill="#185296">
         <font>
            <Font name="Segoe UI Semibold" size="18.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
